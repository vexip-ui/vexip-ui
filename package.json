{
  "author": {
    "name": "qmhc"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "scripts/cz-config.js"
    }
  },
  "dependencies": {
    "@vexip-ui/config": "workspace:^",
    "@vexip-ui/mixins": "workspace:^",
    "@vexip-ui/utils": "workspace:^",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/eslint": "^8.4.2",
    "@types/file-saver": "^2.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.33",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/repl": "^1.0.0",
    "@vue/test-utils": "2.0.0-rc.21",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-customizable": "^6.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "execa": "^5.1.1",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.0",
    "jszip": "^3.9.1",
    "lint-staged": "^12.4.1",
    "minimist": "^1.2.6",
    "pinst": "^3.0.0",
    "postcss": "^8.4.13",
    "postcss-discard-duplicates": "^5.1.0",
    "postcss-html": "^1.4.1",
    "postcss-preset-env": "^7.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "sass": "^1.51.0",
    "semver": "^7.3.7",
    "stylelint": "^14.8.2",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.1.1",
    "vite-plugin-html": "^3.2.0",
    "vue-eslint-parser": "^9.0.1"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "design",
    "dist",
    "icons",
    "lib",
    "style",
    "themes"
  ],
  "homepage": "https://www.vexipui.com",
  "keywords": [
    "flex",
    "components",
    "vue",
    "vexip",
    "plugin",
    "toolkit",
    "vexip-ui",
    "vite"
  ],
  "license": "MIT",
  "main": "dist/vexip-ui.cjs.js",
  "module": "dist/vexip-ui.es.js",
  "name": "vexip-ui",
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qmhc/vexip-ui.git"
  },
  "scripts": {
    "build": "ts-node scripts/build.ts",
    "build:config": "pnpm -C common/config build && pnpm i",
    "build:icons": "ts-node scripts/build-icons.ts",
    "build:lib": "ts-node scripts/build.ts --lib",
    "build:mixins": "pnpm -C common/mixins build && pnpm i",
    "build:playground": "pnpm -C playground build",
    "build:utils": "pnpm -C common/utils build && pnpm i",
    "bootstrap": "ts-node scripts/bootstrap.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "clean": "rimraf node_modules/.cache && rimraf node_modules/.vite && rimraf node_modules/.rts2_cache",
    "commit": "git-cz",
    "create": "ts-node scripts/create.ts",
    "lint": "eslint --fix --ext .js,.ts,.tsx,.vue common/** components/**",
    "lint:style": "stylelint **/*.{vue,scss} --fix",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged -c ./.husky/.lintstagedrc",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write \"{common,component}/**/*.{ts,js,json,css,pcss,scss,vue,html,md}\"",
    "publish:ci": "ts-node scripts/publish.ts",
    "release": "ts-node scripts/release.ts",
    "serve": "ts-node scripts/serve.ts",
    "serve:example": "cd example && vite",
    "serve:playground": "pnpm -C playground serve"
  },
  "sideEffects": [
    "dist/*",
    "lib/**",
    "icons/**",
    "*.scss"
  ],
  "types": "lib/index.d.ts",
  "version": "1.3.2"
}

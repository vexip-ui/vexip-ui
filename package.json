{
  "name": "vexip-ui",
  "version": "2.1.27",
  "type": "module",
  "license": "MIT",
  "author": "qmhc",
  "description": "A Vue 3 UI library, Highly customizability, full TypeScript, performance pretty good",
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "bootstrap": "tsx scripts/bootstrap.ts",
    "props": "tsx scripts/props.ts",
    "serve": "tsx scripts/serve.ts",
    "serve:docs": "pnpm -C docs serve",
    "serve:playground": "pnpm -C playground serve",
    "serve:theme": "tsx scripts/serve.ts --theme",
    "build": "tsx scripts/build.ts",
    "build:bem": "pnpm -C common/bem-helper build && pnpm i",
    "build:common": "pnpm run build:utils && pnpm run build:bem && pnpm run build:hooks && pnpm run build:icons",
    "build:config": "pnpm -C common/config build && pnpm i",
    "build:docs": "pnpm -C docs build",
    "build:full": "tsx scripts/build-full.ts",
    "build:hooks": "pnpm -C common/hooks build && pnpm i",
    "build:icons": "pnpm -C common/icons build && pnpm i",
    "build:meta": "pnpm -C common/meta build && pnpm i",
    "build:playground": "pnpm -C playground build",
    "build:plugins": "pnpm -C common/plugins build && pnpm i",
    "build:style": "gulp --require sucrase/register/ts",
    "build:utils": "pnpm -C common/utils build && pnpm i",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "clean": "rimraf node_modules/.cache && rimraf node_modules/.vite && rimraf node_modules/.rts2_cache",
    "create": "tsx scripts/create.ts",
    "lint": "eslint --cache \"**/*.{js,cjs,mjs,ts,tsx,vue}\"",
    "lint:style": "stylelint --cache \"**/*.{vue,scss}\"",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged -c ./.husky/.lintstagedrc -q --allow-empty",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,json,css,scss,vue,html,md}\"",
    "prettier:scripts": "prettier --write \"scripts/**/*.{ts,js,json}\"",
    "preview:docs": "pnpm -C docs preview",
    "publish:ci": "tsx scripts/publish.ts",
    "release": "tsx scripts/release.ts",
    "test": "vitest run",
    "test:cover": "vitest run --coverage",
    "test:dev": "vitest dev"
  },
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.cjs",
    "./es/*.mjs": "./es/*.mjs",
    "./es/*": "./es/*.mjs",
    "./lib/*.cjs": "./lib/*.cjs",
    "./lib/*": "./lib/*.cjs",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "css/*.css",
    "es/components/*/style.mjs",
    "es/style/*",
    "es/css/*",
    "lib/components/*/style.cjs",
    "lib/style/*",
    "lib/css/*",
    "style/**/*.scss",
    "theme/**/*.css"
  ],
  "files": [
    "css",
    "dist",
    "es",
    "lib",
    "meta-data",
    "style",
    "themes",
    "CHANGELOG.md",
    "meta-data.json",
    "types.d.ts"
  ],
  "keywords": [
    "flex",
    "components",
    "vue",
    "vexip",
    "plugin",
    "toolkit",
    "vexip-ui",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vexip-ui/vexip-ui.git"
  },
  "bugs": {
    "url": "https://github.com/vexip-ui/vexip-ui/issues"
  },
  "homepage": "https://www.vexipui.com",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@vexip-ui/bem-helper": "workspace:~",
    "@vexip-ui/hooks": "workspace:~",
    "@vexip-ui/icons": "workspace:~",
    "@vexip-ui/utils": "workspace:~"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@juggle/resize-observer": "^3.4.0",
    "@types/eslint": "^8.44.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.13",
    "@types/gulp-autoprefixer": "^0.0.34",
    "@types/gulp-clean-css": "^4.3.1",
    "@types/gulp-sass": "^5.0.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.4.1",
    "@types/prettier": "^2.7.3",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.0",
    "@vexip-ui/commitlint-config": "^0.2.0",
    "@vexip-ui/eslint-config": "^0.8.1",
    "@vexip-ui/prettier-config": "^0.2.0",
    "@vexip-ui/stylelint-config": "^0.5.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/test-utils": "^2.4.0",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.44.0",
    "eslint-define-config": "^1.21.0",
    "execa": "^7.1.1",
    "fast-glob": "^3.3.0",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "happy-dom": "10.1.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jszip": "^3.10.1",
    "kolorist": "^1.8.0",
    "lint-staged": "^13.2.3",
    "magic-string": "^0.30.1",
    "minimist": "^1.2.8",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "semver": "^7.5.4",
    "stylelint": "^15.10.1",
    "stylelint-prettier": "^3.0.0",
    "sucrase": "^3.32.0",
    "tsx": "^3.12.7",
    "typescript": "5.0.4",
    "vite": "^4.4.2",
    "vite-plugin-dts": "^3.2.0",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "vue"
      ],
      "allowAny": [
        "@csstools/*"
      ]
    },
    "patchedDependencies": {
      "conventional-changelog-angular@5.0.13": "patches/conventional-changelog-angular@5.0.13.patch",
      "rollup-plugin-visualizer@5.9.2": "patches/rollup-plugin-visualizer@5.9.2.patch"
    }
  }
}

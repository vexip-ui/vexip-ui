{
  "name": "vexip-ui",
  "version": "2.1.16",
  "type": "module",
  "license": "MIT",
  "author": "qmhc",
  "description": "A Vue 3 UI library, Highly customizability, full TypeScript, performance pretty good",
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "bootstrap": "tsx scripts/bootstrap.ts",
    "props": "tsx scripts/props.ts",
    "serve": "tsx scripts/serve.ts",
    "serve:docs": "pnpm -C docs serve",
    "serve:docs-ssr": "pnpm -C docs serve:ssr",
    "serve:playground": "pnpm -C playground serve",
    "serve:theme": "tsx scripts/serve.ts --theme",
    "build": "tsx scripts/build.ts",
    "build:common": "pnpm run build:utils && pnpm run build:hooks && pnpm run build:icons",
    "build:config": "pnpm -C common/config build && pnpm i",
    "build:docs": "pnpm -C docs build",
    "build:full": "tsx scripts/build-full.ts",
    "build:hooks": "pnpm -C common/hooks build && pnpm i",
    "build:icons": "pnpm -C common/icons build && pnpm i",
    "build:playground": "pnpm -C playground build",
    "build:plugins": "pnpm -C common/plugins build && pnpm i",
    "build:style": "gulp --require sucrase/register/ts",
    "build:utils": "pnpm -C common/utils build && pnpm i",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "clean": "rimraf node_modules/.cache && rimraf node_modules/.vite && rimraf node_modules/.rts2_cache",
    "create": "tsx scripts/create.ts",
    "lint": "eslint --ext .js,.ts,.tsx,.vue common/*/src/** components/** directives/** playground/** dev-server/** docs/**",
    "lint:scripts": "eslint --ext .js,.ts scripts/**",
    "lint:style": "stylelint **/*.{vue,scss}",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged -c ./.husky/.lintstagedrc -q",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write \"{common,component,directives,playground,dev-server,docs}/**/*.{ts,js,json,css,pcss,scss,vue,html,md}\"",
    "prettier:scripts": "prettier --write \"scripts/**/*.{ts,js,json}\"",
    "publish:ci": "tsx scripts/publish.ts",
    "release": "tsx scripts/release.ts",
    "test": "vitest run",
    "test:cover": "vitest run --coverage",
    "test:dev": "vitest dev"
  },
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "dist/components/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.cjs",
    "./es/*.mjs": "./es/*.mjs",
    "./es/*": "./es/*.mjs",
    "./lib/*.cjs": "./lib/*.cjs",
    "./lib/*": "./lib/*.cjs",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "css/*.css",
    "es/components/*/style.mjs",
    "es/style/*",
    "es/css/*",
    "lib/components/*/style.cjs",
    "lib/style/*",
    "lib/css/*",
    "style/**/*.scss",
    "theme/**/*.css"
  ],
  "files": [
    "css",
    "dist",
    "es",
    "lib",
    "style",
    "themes",
    "CHANGELOG.md",
    "meta-data.json",
    "types.d.ts"
  ],
  "keywords": [
    "flex",
    "components",
    "vue",
    "vexip",
    "plugin",
    "toolkit",
    "vexip-ui",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vexip-ui/vexip-ui.git"
  },
  "bugs": {
    "url": "https://github.com/vexip-ui/vexip-ui/issues"
  },
  "homepage": "https://www.vexipui.com",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@vexip-ui/hooks": "workspace:^",
    "@vexip-ui/icons": "workspace:^",
    "@vexip-ui/utils": "workspace:^"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@juggle/resize-observer": "^3.4.0",
    "@types/eslint": "^8.21.1",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-sass": "^5.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.14.1",
    "@types/prettier": "^2.7.2",
    "@types/prompts": "^2.4.2",
    "@types/semver": "^7.3.13",
    "@vexip-ui/commitlint-config": "^0.1.0",
    "@vexip-ui/eslint-config": "^0.5.2",
    "@vexip-ui/prettier-config": "^0.1.0",
    "@vexip-ui/stylelint-config": "^0.2.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/repl": "^1.3.2",
    "@vue/test-utils": "^2.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.34.0",
    "execa": "^7.0.0",
    "fast-glob": "^3.2.12",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "happy-dom": "8.7.2",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jszip": "^3.10.1",
    "kolorist": "^1.7.0",
    "lint-staged": "^13.1.2",
    "minimist": "^1.2.8",
    "pinst": "^3.0.0",
    "prettier": "^2.8.4",
    "prompts": "^2.4.2",
    "rimraf": "^4.1.2",
    "sass": "^1.58.3",
    "semver": "^7.3.8",
    "stylelint": "^15.2.0",
    "stylelint-prettier": "^3.0.0",
    "sucrase": "^3.29.0",
    "tsx": "^3.12.3",
    "typescript": "4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.28.5",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vue"
      ],
      "allowAny": [
        "@csstools/*"
      ]
    },
    "patchedDependencies": {
      "conventional-changelog-angular@5.0.13": "patches/conventional-changelog-angular@5.0.13.patch"
    }
  }
}

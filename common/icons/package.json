{
  "name": "@vexip-ui/icons",
  "version": "1.2.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsx scripts/build.ts"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.js"
    },
    "./es/*.mjs": "./es/*.mjs",
    "./es/*": "./es/*.mjs",
    "./lib/*.js": "./lib/*.js",
    "./lib/*": "./lib/*.js",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "types",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vexip-ui/vexip-ui.git",
    "directory": "common/icons"
  },
  "homepage": "https://github.com/vexip-ui/vexip-ui/tree/main/common/icons#readme",
  "devDependencies": {
    "@types/node": "^22.10.6",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.2.0",
    "kolorist": "^1.8.0",
    "prettier": "^3.4.2",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "anchor": "qmhc"
}

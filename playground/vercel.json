{
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "pnpm -w run build:common && pnpm -w run build && pnpm run build",
  "outputDirectory": "dist",
  "github": {
    "silent": true
  },
  "ignoreCommand": "[ \"$VERCEL_ENV\" != production ]"
}
